# API Keys (optional for MVP - uses mock APIs by default)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
SPOTIFY_REDIRECT_URI=http://localhost:8000/api/v1/spotify/callback

# Supabase (optional - only needed if backend reads user data directly)
SUPABASE_URL=https://[YOUR-PROJECT-REF].supabase.co
SUPABASE_SERVICE_KEY=your_supabase_service_role_key_here

# Feature Flags
USE_MOCK_ANTHROPIC=true
USE_MOCK_SPOTIFY=true

# Server Configuration
HOST=0.0.0.0
PORT=8000
LOG_LEVEL=info
FRONTEND_URL=http://localhost:3000
